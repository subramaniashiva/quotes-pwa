{
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": [
    "react",
    "jest"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
	"emitError": true,
  "emitWarning": true,
  "failOnError": true
}