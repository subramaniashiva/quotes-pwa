{
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": [
    "react",
    "jest"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
	"emitError": true,
  "emitWarning": true,
  "failOnError": true,
  rules: {
    "no-console": 1,
    "react/prop-types": [2, {ignore: ["dispatch"]}],
    "comma-dangle": 0,
    quotes: ["error", "single", { "allowTemplateLiterals": true }]
  }
}
